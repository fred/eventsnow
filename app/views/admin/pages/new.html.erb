<h2><%= @title = "New Page" %></h2>

<% form_for(@page, :url => admin_pages_path, :html => { :multipart => true }) do |f| %>

  <%= f.error_messages %>
  
  <%= render :partial => 'form', :locals => { :f => f, :page => @page } %>

  <p>
    <%= f.submit 'Save' %>
    <input type="reset" value="Reset">
  </p>

<% end %>

<%= link_to 'Back', admin_pages_path %>

